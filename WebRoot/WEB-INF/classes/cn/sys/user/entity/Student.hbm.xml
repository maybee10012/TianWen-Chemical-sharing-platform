<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.sys.user.entity.Student" table="student">
		<id name="id" type="java.lang.String">
			<column name="id" length="30" />
			
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="10" not-null="true" />
		</property>
		<property name="username" type="java.lang.String">
			<column name="username" length="20" not-null="true" />
		</property>		
		<property name="gender" type="java.lang.String">
			<column name="gender" length="10" not-null="true" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" length="20" not-null="true" />
		</property>
		<property name="headImg" type="java.lang.String">
		   <column name="headImg" length="50" not-null="false"></column>
		</property>
		<property name="phone" type="java.lang.String">
			<column name="phone"  length="20" not-null="true"/>
		</property>
		<property name="state" type="java.lang.String">
			<column name="state" length="10"/>
		</property>
		<property name="workhouse" type="java.lang.String">
			<column name="workhouse" length="20" />
		</property>
		<property name="email" type="java.lang.String">
			<column name="email" length="20" />
		</property>
		<property name="profession" type="java.lang.String">
			<column name="profession" length="20" />
		</property>
		<property name="registdate" type="java.util.Date">
		    <column name="registdate" length="10"></column>
		</property>
		<property name="teacher" type="java.lang.String">
			<column name="teacher" length="20" />
		</property>
		<property name="college" type="java.lang.String">
			<column name="college" length="20" />
		</property>
	</class>

</hibernate-mapping>
	